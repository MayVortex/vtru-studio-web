{
    "context": {
        "formData": {
            "tags": ["artwork"],
            "moods": []
        },
        "schema": {
            "type": "object",
            "required": ["title", "description", "tags", "moods"],
            "properties": {
                "title": {
                    "type": "string",
                    "title": "title",
                    "description": "THE Title of work"
                },
                "description": {
                    "type": "string",
                    "format": "textarea",
                    "title": "description",
                    "description": "Brief description of work"
                },
                "tags": {
                    "type": "array",
                    "title": "tag",
                    "minItems": 1,
                    "description": "Tags relevant to the work",
                    "items": {
                        "type": "string"
                    }
                },
                "moods": {
                    "type": "array",
                    "title": "moods",
                    "minItems": 1,
                    "description": "Feelings that the work evokes",
                    "uniqueItems": true,
                    "items": {
                        "type": "string",
                        "enum": [
                            "Admiration",
                            "Absorbing",
                            "Amusement",
                            "Adoration",
                            "Awe",
                            "Anxiety",
                            "Boredom",
                            "Brooding",
                            "Calmness",
                            "Chills",
                            "Chaotic",
                            "Connectedness",
                            "Cosmic",
                            "Confusion",
                            "Dread",
                            "Distaste",
                            "Disgust",
                            "Dreary",
                            "Disorienting",
                            "Dreamy",
                            "Desire",
                            "Elegant",
                            "Humorous",
                            "Intimate",
                            "Intricate",
                            "Love",
                            "Lively",
                            "Mystical",
                            "Mysterious",
                            "Nostalgia",
                            "Ornate",
                            "Psychedelic",
                            "Serenity",
                            "Sadness",
                            "Sensual",
                            "Spiritual",
                            "Strange",
                            "Striking",
                            "Tragic",
                            "Tense",
                            "Vibrant",
                            "Violent",
                            "Wonder",
                            "Whimsical"
                        ]
                    }
                }
            }
        },
        "uiSchema": {
            "title": {
                "classNames": "my-custom-class",
                "ui:options": {
                    "label": false,
                    "placeholder": ""
                }
            },
            "description": {
                "classNames": "my-custom-class",
                "ui:widget": "textarea",
                "ui:options": {
                    "label": false,
                    "placeholder": ""
                }
            },
            "tags": {
                "items": {
                    "ui:emptyValue": "",
                    "ui:options": {
                        "label": false
                    }
                },
                "ui:options": {
                    "label": false
                }
            },
            "moods": {
                "ui:widget": "checkboxes",
                "ui:field": "noTitle",
                "ui:title": false,
                "ui:options": {
                    "label": false
                },
                "items": {
                    "ui:options": {
                        "label": false
                    }
                }
            }
        }
